<view class="sunui-stars">
    <view class="sunui-m">
        <view bindtap="__e" class="sunui-stars-items" data-event-opts="{{[ [ 'tap',[ [ 'changeStar',['$event'] ] ] ] ]}}" data-value="{{index}}" wx:for="{{maxStar}}" wx:key="index">
            <text class="{{['iconfont',iconName,curStarNum>index?'icon-star-hover':'icon-star-nhover']}}" style="{{'font-size:'+starSize+';'}}"></text>
        </view>
    </view>
    <slot></slot>
</view>
