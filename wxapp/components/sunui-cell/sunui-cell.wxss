@font-face {
    font-family: sunuicell;
    src: url(//at.alicdn.com/t/iconfont.eot?t=1578706076680);
    src: url(//at.alicdn.com/t/iconfont.eot?t=1578706076680#iefix) format("embedded-opentype"),url("data:application/x-font-woff2;charset=utf-8;base64,") format("woff2"),url(//at.alicdn.com/t/iconfont.woff?t=1578706076680) format("woff"),url(//at.alicdn.com/t/iconfont.ttf?t=1578706076680) format("truetype"),url(//at.alicdn.com/t/iconfont.svg?t=1578706076680#iconfont) format("svg");
