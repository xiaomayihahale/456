.sunuicell {
    font-family: sunuicell!important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.icon-youjiantou:before {
    content: "\e630";
}

.sunui-cell {
    position: relative;
    width: 100%;
    box-sizing: border-box;
    overflow: hidden;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    padding: 10rpx 0rpx;
}

.sunui-cell>.icon-youjiantou {
    font-weight: 700;
    position: absolute;
    right: 20rpx;
}

.sunui-cell-hover {
    background-color: #f5f5f5!important;
    box-shadow: 0 0 4rpx #f5f5f5;
}