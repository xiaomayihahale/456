<view class="line_chart">
    <canvas bindtouchend="__e" bindtouchmove="__e" bindtouchstart="__e" canvasId="{{canvasId}}" data-event-opts="{{[ [ 'touchstart',[ [ 'touchstart',['$event'] ] ] ],[ 'touchmove',[ [ 'touchmove',['$event'] ] ] ],[ 'touchend',[ [ 'touchend',['$event'] ] ] ] ]}}" disableScroll="true" id="canvasId" style="{{'width:'+cWidth+'px'+';'+'height:'+cHeight+'px'+';'}}"></canvas>
    <slot></slot>
</view>
