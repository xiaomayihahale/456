button::after {
    border: none;
}

page {
    background-color: #f7f7f7;
}

view {
    font-size: 28rpx;
    color: #000;
}

.address_img {
    font-size: 36rpx;
    color: #fff;
    width: 96rpx;
    height: 96rpx;
    background-image: -webkit-gradient(linear,left top,right top,from(#cacaca),to(#aeaeae));
    background-image: -webkit-linear-gradient(left,#cacaca,#aeaeae);
    background-image: linear-gradient(90deg,#cacaca,#aeaeae);
    border-radius: 50%;
    margin-left: 20rpx;
}

.realname {
    width: 132rpx;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.address_text {
    color: #a1a1a1;
    font-size: 24rpx;
    margin: 14rpx 0rpx 30rpx;
}

.button_address {
    position: fixed;
    bottom: 40rpx;
    width: 750rpx;
}

.address_btn {
    padding-top: 8rpx;
    width: 680rpx;
    height: 96rpx;
    background-image: -webkit-gradient(linear,left top,right top,color-stop(1.86%,#ffba64),color-stop(93.76%,#f60));
    background-image: -webkit-linear-gradient(left,#ffba64 1.86%,#f60 93.76%);
    background-image: linear-gradient(90deg,#ffba64 1.86%,#f60 93.76%);
    background-color: #689ceb;
    border-radius: 50rpx;
    color: #fff;
    font-size: 32rpx;
    text-align: center;
}

.address {
    background-color: #fff;
    width: 750rpx;
    margin-top: 20rpx;
}

.line {
    width: 2rpx;
    height: 70rpx;
    background-color: #c9c9c9;
    margin-right: 22rpx;
}