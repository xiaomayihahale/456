.uni-numbox--disabled.uni-numbox__minus {
    color: #666!important;
    border: 1rpx solid #fff;
}

.uni-numbox--disabled {
    color: #666!important;
    background-color: initial!important;
    border: 2rpx solid #999!important;
}

.uni-numbox {
    display: -webkit-inline-box;
    display: -webkit-inline-flex;
    display: inline-flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    flex-direction: row;
    -webkit-box-pack: start;
    justify-content: flex-start;
    height: 42rpx;
    position: relative;
}

.uni-numbox__minus,.uni-numbox__plus {
    margin: 0;
    background-color: #f8f8f8;
    width: 48rpx;
    font-size: 40rpx;
    height: 100%;
    line-height: 42rpx;
    text-align: center;
    display: -webkit-inline-box;
    display: -webkit-inline-flex;
    display: inline-flex;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: center;
    justify-content: center;
    color: #333;
    position: relative;
}

.uni-numbox__minus {
    color: #d7d7d7!important;
    border: 1rpx solid #d7d7d7;
    background-color: #fff;
}

.uni-numbox__plus {
    color: #d7d7d7!important;
    border: 1rpx solid #d7d7d7;
}

.uni-numbox__value {
    position: relative;
    background-color: #fff;
    width: 70rpx;
    height: 100%;
    text-align: center;
    padding: 0;
    line-height: 42rpx;
}