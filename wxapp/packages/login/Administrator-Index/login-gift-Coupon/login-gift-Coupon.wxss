.sunui-coupons-margin {
    padding: 20rpx;
}

.sunui-coupons-margin>.sunui-coupons {
    margin-bottom: 30rpx;
}

.sunui-coupons {
    display: flex;
    flex-wrap: nowrap;
    border-top-left-radius: 10rpx;
    border-bottom-left-radius: 10rpx;
}

.sunui-coupons .left {
    width: 100%;
    height: 150rpx;
    padding-right: 10rpx;
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    -webkit-box-align: center;
    align-items: center;
    border-radius: 8rpx;
    position: relative;
    background-color: #fff;
    height: 180rpx;
}

.sunui-coupons .left .number {
    width: 66%;
    display: flex;
    flex-wrap: wrap;
    border-right: 1px dashed #d2d2d2;
}

.sunui-coupons .left .number .show {
    font-size: 50rpx;
    font-weight: 700;
    line-height: 66rpx;
    text-align: center;
    width: 100%;
}

.sunui-coupons .left .number text {
    color: #828282;
    font-weight: 400;
    font-size: 50rpx;
}

.sunui-coupons .left .number .txt {
    line-height: 26rpx;
    font-size: 22rpx;
    text-align: center;
    width: 100%;
}

.sunui-coupons .left .body {
    width: 62%;
    margin-left: 4%;
    display: flex;
    flex-wrap: wrap;
    position: relative;
}

.sunui-coupons .left .status {
    display: inline-block;
    color: #fff;
    padding: 0 10px;
    height: 36rpx;
    line-height: 36rpx;
    font-size: 20rpx;
    position: absolute;
    z-index: 1;
    right: 6px;
    top: 8px;
    border-radius: 36rpx;
}

.sunui-coupons .left .body .title {
    width: 100%;
    font-size: 30rpx;
    line-height: 46rpx;
}

.sunui-coupons .left .body .desc {
    color: #999;
    margin-top: 3px;
    font-size: 24rpx;
    line-height: 36rpx;
}

.sunui-coupons .right {
    width: 150rpx;
    height: 150rpx;
    flex-shrink: 0;
    font-size: 30rpx;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: center;
    justify-content: center;
    color: #fff;
}

.sunui-coupons .sawtooth {
    width: 8px;
    height: 150rpx;
    overflow: hidden;
    position: relative;
    flex-shrink: 0;
}

.sunui-coupons .sawtooth .circular {
    width: 10px;
    margin: 5px;
    height: 10px;
    border-radius: 10px;
    background: #f9f9f9;
}

.aaa {
    width: 33%;
    height: 80rpx;
    text-align: center;
    padding: 0rpx 50rpx;
    color: #a0cfff;
    display: inline-block;
    position: relative;
    font-size: 28rpx;
}

.nav-item {
    width: 33%;
    height: 80rpx;
    text-align: center;
    padding: 0rpx 50rpx;
    color: #fff;
    display: inline-block;
    position: relative;
    font-size: 28rpx;
}

.nav-item .title {
    line-height: 90rpx;
    border-bottom: 4rpx solid #fff;
    padding-bottom: 10rpx;
    font-weight: 700;
}

.nav-item:after {
    content: "";
    width: 0;
    height: 0;
    border-bottom: 6rpx solid #fff;
    position: absolute;
    left: 50%;
    bottom: 0;
    transform: translateX(-50%);
    transition: .3s;
}

