page {
    background: #f0f0f0;
}

.all {
    position: relative;
    width: 712rpx;
    height: auto;
    padding: 24rpx 0rpx 24rpx 36rpx;
    background-color: #fff;
    border-radius: 12rpx;
    margin-top: 24rpx;
}

.img {
    width: 64rpx;
    height: 64rpx;
    border-radius: 50%;
    margin-right: 20rpx;
}

.title {
    font-size: 32rpx;
    font-weight: 700;
}

.delete {
    width: 98rpx;
    height: 46rpx;
    position: absolute;
    top: 0rpx;
    right: 0rpx;
    background: #ff4d4d;
    border-radius: 40rpx 0rpx 0rpx 40rpx;
    color: #fff;
    font-size: 24rpx;
}

.text {
    font-size: 24rpx;
    color: #666;
}

.edit {
    width: 132rpx;
    height: 56rpx;
    border: 1px solid #689ceb;
    border-radius: 78rpx;
    font-weight: 700;
    font-size: 24rpx;
    color: #6b8ad3;
    margin-right: 14rpx;
}